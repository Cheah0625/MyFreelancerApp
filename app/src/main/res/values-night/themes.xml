<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.MyFreelancer" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/dark_blue2</item>
        <item name="colorPrimaryVariant">@color/dark_blue</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/dark_blue2</item>
        <item name="colorSecondaryVariant">@color/dark_blue2</item>
        <item name="colorOnSecondary">@color/white</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">@color/dark_blue2</item>
        <!-- Customize your theme here. -->

        <item name="fontFamily">@font/roboto</item>
    </style>

    <style name="circular">
        <item name="cornerSize">50%</item>
    </style>

</resources>